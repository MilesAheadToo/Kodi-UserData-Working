  "default_profile": "UK",

  "profiles": {
    "UK": { "slot": 1, "label": "UK (UDP)" },
    "DE": { "slot": 2, "label": "DE (UDP)" },
    "CA": { "slot": 3, "label": "CA (UDP)" },
    "US": { "slot": 4, "label": "US (UDP)" }
  },

  "mappings": {
