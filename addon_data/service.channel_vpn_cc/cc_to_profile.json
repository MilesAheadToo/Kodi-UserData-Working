      "UK": "UK",
      "GB": "UK",
      "uk": "UK",
      "gb": "UK",

      "DE": "DE",
      "de": "DE",

      "CA": "CA",
      "ca": "CA",

      "US": "US",
      "us": "US"
    },

    "addon_overrides": {
      "plugin.video.iplayerwww": "UK"
    },

    "channel_overrides": {
    }
  },

  "logging": {
    "enabled": true,
    "path": "special://profile/addon_data/service.channel_vpn_cc/service.log"
  }
}
